   PROCEDURE Sauvegarder_Tab_Fiche(T : T_TabFiche; Nom_Fichier : STRING);

   PROCEDURE Restaurer_Tab_Fiche(T : OUT T_TabFiche; Nom_Fichier : STRING);
   
   PROCEDURE Sauvegarder_Tab(TG : T_TabGroupe; Nom_Fichier : STRING);
   
   PROCEDURE Restaurer_Tab(TG : OUT T_TabGroupe; Nom_Fichier : STRING);
